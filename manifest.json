{
  "manifest_version": 2,

  "name": "Domain Switch",
  "description": "This extension switches domains while keeping the path of the current url",
  "version": "1.0",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon-16.png",
    "default_popup": "popup.html"
  }
}